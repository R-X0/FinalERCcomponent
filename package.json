{
    "name": "erc-dashboard-app",
    "version": "1.0.0",
    "description": "ERC Dashboard with Express backend and React frontend",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "client": "cd erc-dashboard && npm start",
        "dev": "concurrently \"npm start\" \"npm run client\"",
        "build": "cd erc-dashboard && npm run build",
        "install-all": "npm install && cd erc-dashboard && npm install"
    },
    "dependencies": {
        "cheerio": "^1.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "mongoose": "^8.12.1",
        "puppeteer": "^24.4.0"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}
